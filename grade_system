def grade_system():
    total = 0

    for i in range(5):
        mark = float(input("Enter mark: "))
        total += mark
    average = total / 5
    print("Average:", average)

    if average >= 80:
        print("Grade: A")
    elif average >= 70:
        print("Grade: B")
    elif average >= 60:
        print("Grade: C")
    elif average >= 50:
        print("Grade: D")
    else:
        print("Grade: F")
grade_system()
